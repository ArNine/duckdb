[
    {
        "name": "strftime",
        "parameters": "data,format",
        "description": "Converts a `date` to a string according to the format string.",
        "example": "strftime(date '1992-01-01', '%a, %-d %B %Y')",
        "type": "scalar_function_set",
        "struct": "StrfTimeFun"
